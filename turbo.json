{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build", "^db:generate"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["build/**", "dist/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false,
			"dependsOn": ["^db:generate"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"format": {
			"dependsOn": ["^format"]
		},
		"db:generate": {
			"cache": false,
			"dependsOn": ["^db:generate"]
		},
		"db:migrate": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["^db:migrate"]
		},
		"db:seed": {
			"cache": false,
			"dependsOn": ["^db:seed"]
		},
		"db:studio": {
			"cache": false,
			"dependsOn": ["^db:studio"]
		}
	}
}
